
    @prefix tree: <https://w3id.org/tree#> .
    @prefix ldes: <https://w3id.org/ldes#> .
    @prefix dc: <http://purl.org/dc/terms/> .
    @prefix sh: <http://www.w3.org/ns/shacl#> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
    @prefix skos: <http://www.w3.org/2004/02/skos/core#> .

    @base <https://marine-term-translations.github.io/P02-NL/LDES/1756365514.ttl> .
    
    <>
        a tree:Node ;
        
        ldes:retentionPolicy [
            a ldes:LatestVersionSubset ;
            ldes:amount 100 ;
            ldes:versionKey ( dc:isVersionOf )
        ] .
    
    <../feed.ttl>
      a ldes:EventStream ;
      ldes:timestampPath dc:modified ;
      ldes:versionOfPath dc:isVersionOf ;
      tree:shape [
        a sh:NodeShape ;
        sh:nodeKind sh:IRI ;
        sh:property [
          sh:datatype xsd:string ;
          sh:minCount 1 ;
          sh:path <http://www.w3.org/2004/02/skos/core#definition> 
        ];
        sh:property [
          sh:datatype xsd:string ;
          sh:minCount 1 ;
          sh:path <http://www.w3.org/2004/02/skos/core#altLabel> 
        ];
        sh:property [
          sh:datatype xsd:string ;
          sh:minCount 1 ;
          sh:path <http://www.w3.org/2004/02/skos/core#prefLabel> 
        ];
        sh:property [
          sh:datatype xsd:string ;
          sh:minCount 1 ;
          sh:path dc:isVersionOf
        ];
        
        sh:targetClass skos:Concept
      ] ;
      tree:view <> ;
      .
    